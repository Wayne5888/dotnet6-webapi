{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/{Date}.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{Replace InstrumentationKey For Azure ApplicationInsights}"
  },
  "ConnectionStrings": {
    "DefaultConnection": "{Replace to your database ConnectionStrings}"
  },
  "AllowedHosts": "*"
}
